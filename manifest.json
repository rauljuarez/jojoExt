
{
  "name": "JOJOLEJO v 0.0.1",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": ["tabs", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["*://*.tripadvisor.com/*"],
      "js": ["jquery-2.1.1.min.js", "contentscripts.js"],
      "run_at": "document_end"
    }
  ]
}